<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WheelDefender</title>

    <!-- ===== ===== Custom Css ===== ===== -->
    <link rel="stylesheet" href="../css/drivercss.css">

    <!-- ===== ===== Remix Font Icons Cdn ===== ===== -->
    <link rel="stylesheet" href="fonts/remixicon.css">
</head>

<body>
    <!-- ===== ===== Body Main-Background ===== ===== -->
    <span class="main_bg"></span>


    <!-- ===== ===== Main-Container ===== ===== -->
    <div class="container">

        <!-- ===== ===== Header/Navbar ===== ===== -->
        <header>
            <div class="brandLogo">
                <span>WheelDefender</span>
            </div>
        </header>


        <!-- ===== ===== User Main-Profile ===== ===== -->
        <section class="userProfile card">
            <div class="profile">
                <figure><img src="" alt="profile" width="250px" height="250px"></figure>
            </div>
        </section>

        <section class="work_skills card">
            <div class="work">
                <div class="primary">
                    <h1><%= data.vehicleManufacturerName %></h1>
                    <p><%= data.model %> <br> New Delhi,Dl52</p>
                </div>

                <div class="secondary">
                    <h1>AzadPur <br> Karnal Road</h1>
                    <p>Shastri Nagar <br> New Delhi,Dl52</p>
                </div>
            </div>

            <div class="circle-wrap">
                <div class="circle">

                    <div class="mask full">
                        <div class="fill"></div>
                    </div>

                    <div class="mask half">
                        <div class="fill"></div>
                    </div>

                    <div class="inside-circle">
                        80 points
                    </div>


                </div>
            </div>
        </section>


        <!-- ===== ===== User Details Sections ===== ===== -->
        <section class="userDetails card">
            <div class="userName">
                <h1 class="name">Krati</h1>
                <p>Ola</p>
            </div>
            <div class="btns">
                <ul>
                    <li class="sendMsg">
                        <i class="ri-chat-4-fill ri"></i>
                        <a href="#">Send Message</a>
                    </li>

                    <li class="sendMsg active">
                        <i class="ri-check-fill ri"></i>
                        <a href="#">Contacts</a>
                    </li>

                    <li class="sendMsg">
                        <a href="#">Report User</a>
                    </li>
                </ul>
            </div>
        </section>


        <!-- ===== ===== Timeline & About Sections ===== ===== -->
        <section class="timeline_about card">
            <div class="tabs">
                <ul>
                    <li class="timeline">
                        <i class="ri-eye-fill ri"></i>
                        <span>Timeline</span>
                    </li>

                    <li class="about active">
                        <i class="ri-user-3-fill ri"></i>
                        <span>About</span>
                    </li>
                </ul>
            </div>

            <div class="contact_Info">
                <h1 class="heading">Contact Information</h1>
                <ul>
                    <li class="phone">
                        <h1 class="label">Phone:</h1>
                        <span class="info">+91 7300696475</span>
                    </li>
                    <li class="dl">
                        <h1 class="label">Driving Licence:</h1>
                        <span class="info">ghj</span>
                    </li>
                    <li class="address">
                        <h1 class="label">Address:</h1>
                        <span class="info">Shastri Nagar <br> New Delhi,Dl52</span>
                    </li>

                    <li class="email">
                        <h1 class="label">E-mail:</h1>
                        <span class="info">manvendra.singh_cs20@gla.ac.in</span>
                    </li>
                </ul>
            </div>

            <div class="basic_info">
                <h1 class="heading">Basic Information</h1>
                <ul>
                    <li class="birthday">
                        <h1 class="label">Birthday:</h1>
                        <span class="info">jhbhv</span>
                    </li>

                    <li class="sex">
                        <h1 class="label">Gender:</h1>
                        <span class="info">Male</span>
                    </li>
                </ul>
            </div>
        </section>
    </div>

</body>

</html>